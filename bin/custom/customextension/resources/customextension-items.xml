<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="items.xsd">

    <itemtypes>
        <itemtype code="Product" generate="false" autocreate="false">
            <attributes>
                <attribute qualifier="additionalDescription" type="localized:java.lang.String">
                    <description>Additional Product Description</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>
        <itemtype code="ProductBundle" extends="Product" generate="true" autocreate="true">
            <attributes>
                <attribute qualifier="productsList" type="ProductCollection">
                    <description>Additional Product Description</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>
    </itemtypes>
</items>
